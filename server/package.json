{
  "name": "relay-modern-crud",
  "version": "1.0.0",
  "description": "CRUD operation using relay modern and graphql with prisma",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel index.js",
    "prisma": "prisma init pserver",
    "prisma-deploy": "cd pserver && prisma deploy",
    "prisma-up": "cd pserver && docker-compose up",
    "relay": "relay-compiler --src ./src --schema ./src/schema/schema.graphql --watch"
  },
